   "source": [
    "import numpy as np\n",
    "\n",
    "P = np.array([\n",
    "    [0.4, 0.6],\n",
    "    [0.2, 0.8]\n",
    "])\n",
    "\n",
    "T = 10\n",
    "\n",
    "print(\"P^t:\")\n",
    "for t in range(1, T + 1):\n",
    "    Pt = np.linalg.matrix_power(P, t)\n",
    "    print(f\"\\nP^{t} =\")\n",
    "    print(np.round(Pt, 6))\n"
   ]
  }
